{
  "name": "akita-react-hooks-ts",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "keywords": [
    "React",
    "React-Router",
    "Akita",
    "TypeScript",
    "RxJS",
    "ES6",
    "React-Boilerplate",
    "React-Starter-Kit"
  ],
  "dependencies": {
    "@datorama/akita": "^4.19.2",
    "@types/node": "13.7.2",
    "@types/react": "16.9.20",
    "@types/react-dom": "16.9.5",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-to-json": "^3.3.5",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.1",
    "react-markdown": "^4.0.8",
    "react-router-dom": "^5.0.1",
    "react-scripts": "3.4.0",
    "rxjs": "^6.5.2",
    "rxjs-compat": "^6.5.2",
    "snapshot-diff": "^0.7.0",
    "styled-components": "^5.0.1",
    "styled-minimal": "^0.9.0",
    "typescript": "3.7.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:coverage": "yarn test --coverage --logHeapUsage --no-cache",
    "test:leak": "yarn test --detectLeaks",
    "eject": "react-scripts eject",
    "lint": "tslint -c tslint.json src/**/*.{ts,tsx} && eslint src/**/*.{js,jsx}",
    "prettier": "prettier \"**/*.{ts,tsx,scss,css,js,json,md}\" --write",
    "flow": "flow",
    "flow:watch": "flow-watch",
    "flow:install": "flow-typed install -o"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && yarn lint",
      "pre-push": "yarn test --no-watch"
    }
  },
  "lint-staged": {
    "*.{ts,scss,css,js,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "akitaCli": {
    "customFolderName": "true",
    "isAngular": false,
    "basePath": "./src/core/"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@datorama/akita-cli": "^6.1.0",
    "@typescript-eslint/parser": "^2.20.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.10.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-flowtype": "^4.6.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jest": "^23.7.0",
    "eslint-plugin-jsx-control-statements": "^2.2.1",
    "eslint-plugin-react-hooks": "^2.4.0",
    "flow-bin": "^0.118.0",
    "flow-typed": "^3.0.0",
    "flow-watch": "^1.1.4",
    "husky": "^4.2.3",
    "lint-staged": "^10.0.7",
    "prettier": "^1.18.2",
    "ts-jest": "^25.2.0",
    "tslint": "^6.0.0",
    "tslint-config-react-app": "^0.0.0",
    "tslint-react": "^4.0.0"
  }
}
